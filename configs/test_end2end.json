{
  "experiment": {
    "name": "test_end2end_val_after_train"
  },

  "data": {
    "noise_config": {
        "label_flip": {"ratio": 0.2},
        "gaussian": {"std": 0.05},
        "salt_pepper": {"amount": 0.02},
        "contrast": {"factor_range": [0.7, 1.3]},
        "rotate": {"angle_range": 10},
        "blur": {"kernel_size": 3},
        "brightness": {"shift_range": 0.2}
      },
    "test_size": 0.2,
    "random_state": 42,
    "use_feature": "hu",        

    "hu_params": {
      "log_scale": true
    }
  },

  "monitor": {
    "is_data_noisy": true,
    "checkpoint_interval": 10,
    "val_freq": 10,
    "val_n_jobs": 8
  },

  "model": {
    "estimator": {
      "max_depth": 2
    },
    "n_estimators": 50,
    "learning_rate": 0.5,
    "random_state": 42
  }
}
