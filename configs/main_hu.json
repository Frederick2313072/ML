{
  "experiment": {
    "name": "main_hu"  
  },

  "data": {
    "noise_ratio": 0.0,            
    "test_size": 0.2,             
    "random_state": 42,          
    "use_feature": "hu",        

    "hu_params": {
      "log_scale": true
    }
  },

  "monitor": {
    "use_monitor": true,
    "is_data_noisy": false,
    "checkpoint_interval": 100
  },
  "model": {
    "estimator": {
      "max_depth": 2,         
      "criterion": "entropy",
      "max_features": 0.25, 
      "random_state": 42
    },
    "n_estimators": 1000,   
    "learning_rate": 0.5, 
    "random_state": 42   
  }
}
