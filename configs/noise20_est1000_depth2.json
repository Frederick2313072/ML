{
  "experiment": {
    "name": "noise20_est1000_depth2"
  },

  "data": {
    "noise_ratio": 0.20,
    "test_size": 0.2,
    "use_feature": "original",
    "random_state": 42
  },

  "monitor": {
    "use_monitor": true,
    "is_data_noisy": true,
    "checkpoint_interval": 100
  },

  "model": {
    "estimator": {
      "max_depth": 2,
      "criterion": "entropy",       
      "max_features": 0.25
    },
    "n_estimators": 1000,
    "learning_rate": 0.5,
    "random_state": 42
  }
}
